{
  "name": "@studysync/eslint-plugin-persnickety",
  "version": "1.0.4",
  "private": false,
  "description": "Extremely picky eslint rules.",
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha '**/*.test.js' --exclude '**/node_modules/**'",
    "test:watch": "cross-env NODE_ENV=test mocha '**/*.test.js' --verbose --watch --exclude '**/node_modules/**' --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dkadrios/eslint-plugin-persnickety.git"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-eslint": ">= 10",
    "cross-env": ">= 7",
    "eslint": ">= 6",
    "mocha": ">= 7"
  },
  "dependencies": {},
  "peerDependencies": {
    "eslint": ">= 6.0.0"
  }
}
